<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="288.0" prefWidth="289.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kalkulator.CalculatorController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

   <TextField fx:id="display" onAction="#handleButtonClick" />
   <VBox>
      <children>
         <GridPane alignment="BASELINE_CENTER" hgap="5.0" minHeight="-Infinity" vgap="5.0">
           <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               <RowConstraints maxHeight="48.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="/">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="*" GridPane.columnIndex="1">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="-" GridPane.columnIndex="2">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="7" GridPane.rowIndex="1">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="8" GridPane.columnIndex="1" GridPane.rowIndex="1">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="9" GridPane.columnIndex="2" GridPane.rowIndex="1">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="4" GridPane.rowIndex="2">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="5" GridPane.columnIndex="1" GridPane.rowIndex="2">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="6" GridPane.columnIndex="2" GridPane.rowIndex="2">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="1" GridPane.rowIndex="3">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="2" GridPane.columnIndex="1" GridPane.rowIndex="3">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" text="3" GridPane.columnIndex="2" GridPane.rowIndex="3">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="\%" GridPane.columnIndex="3">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="." GridPane.columnIndex="2" GridPane.rowIndex="4">
                  <font>
                     <Font size="14.0" />
                  </font></Button>
               <Button alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" minHeight="65.0" minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="+" textAlignment="JUSTIFY" GridPane.columnIndex="3" GridPane.rowIndex="1" GridPane.rowSpan="2">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Button>
               <Button minHeight="65.0" minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="=" GridPane.columnIndex="3" GridPane.rowIndex="3" GridPane.rowSpan="2">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="clr" GridPane.rowIndex="4">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Button>
               <Button minWidth="30.0" mnemonicParsing="false" onAction="#handleButtonClick" text="0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Button>
            </children>
         </GridPane>
      </children>
   </VBox>
</VBox>
